    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "bullmq": "^3.5.1",
    "eslint": "^8.23.0",
    "ioredis-mock": "^8.2.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rewiremock": "^3.14.3",
    "shimmer": "^1.2.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenniferplusplus/opentelemetry-instrumentation-bullmq.git"
